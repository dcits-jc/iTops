<ol class="breadcrumb">
  <li><%= link_to("全部用户",admin_users_path) %></li>
  <li class="active"><%= @user.email %></li>
</ol>


<h1><%= @user.name %></h1>
<p>员工编号:<%= @user.code %></p>
<p>ITcode:<%= @user.itcode %></p>
<p>所属部门:<%= @user.sbu %></p>
<p>联系方式:<%= @user.phone %></p>
<p>职位:<%= @user.title %></p>
<p>级别:<%= @user.level %></p>
<p>成本:<%= @user.cost %></p>



<%= link_to('编辑',edit_admin_user_path(@user),class: "btn btn-primary") %>



<table class="table">
  <thead>
    <tr>
      <td>周</td>
    </tr>
  </thead>
  <tbody>
    <% @user.weeklies.each do |weekly| %>
    <tr>
      <td><%= link_to('Y'+(weekly.year).to_s+'W'+(weekly.week).to_s,admin_user_weekly_path(@user,weekly))  %></td>
    </tr>
    
    <% end %>
  </tbody>
</table>


