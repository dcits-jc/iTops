<%#= link_to('新建周报模板',new_admin_weekly_template_path) %>


<table class="table">
  <thead>
    <tr>
      <td>日期</td>
      <td>时间</td>
      <td>提交人数(人)</td>
      <td>合计工作量(小时)</td>
      <td>合计费用(元)</td>
    </tr>
  </thead>
  <tbody>
    <% @weekly_templates.each do |weekly_template| %>
    <tr>
      <td><%= link_to 'Y'+weekly_template.year.to_s+'-W'+weekly_template.week.to_s,admin_weekly_template_path(weekly_template) %></td>
      <td><%= weekly_template.start_time.strftime("%Y-%m-%d") %> ~ <%= weekly_template.end_time.strftime("%Y-%m-%d") %></td>
      <td><%= weekly_template.user_submitcount %></td>
      <td><%= weekly_template.weekly_workload_total %></td>
      <td><%= weekly_template.weekly_cost_total %></td>
    </tr>
    <% end %>
  </tbody>
</table>