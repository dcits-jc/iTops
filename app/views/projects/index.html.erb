<%= link_to('新建项目',new_project_path,class: "btn btn-primary") %>

<table class="table table-striped">
  <thead>
    <tr>
      <td>项目名</td>
      <td>项目描述</td>
    </tr>
  </thead>
  <tbody>
    <% @projects.each do |project| %>
    <tr>
      <td><%= link_to(project.name,project_path(project)) %></td>
      <td><%= project.description %></td>
    </tr>
    <% end %>
  </tbody>
</table>